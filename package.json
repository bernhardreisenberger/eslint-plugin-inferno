{
  "name": "eslint-plugin-inferno",
  "version": "7.11.0",
  "description": "Inferno specific linting rules for ESLint",
  "main": "index.js",
  "scripts": {
    "coveralls": "cat ./reports/coverage/lcov.info | coveralls",
    "lint": "eslint ./",
    "pretest": "npm run lint",
    "test": "npm run unit-test",
    "unit-test": "istanbul cover --dir reports/coverage node_modules/mocha/bin/_mocha tests/**/*.js -- --reporter dot"
  },
  "files": [
    "LICENSE",
    "README.md",
    "index.js",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/infernojs/eslint-plugin-inferno"
  },
  "homepage": "https://github.com/infernojs/eslint-plugin-inferno",
  "bugs": "https://github.com/infernojs/eslint-plugin-inferno",
  "dependencies": {
    "doctrine": "^2.1.0",
    "has": "^1.0.3",
    "jsx-ast-utils": "^2.0.1",
    "prop-types": "^15.6.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "coveralls": "^3.0.2",
    "eslint": "^5.7.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0"
  },
  "peerDependencies": {
    "eslint": "^5.7.0"
  },
  "engines": {
    "node": ">=4"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslintplugin",
    "inferno"
  ],
  "license": "MIT"
}
